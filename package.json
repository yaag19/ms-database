    "morgan": "^1.4.1",
    "mysql": "^2.18.1",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "@mochajs/json-file-reporter": "^1.3.0",
    "chai": "^3.5.0",
    "colors": "^1.3.3",
    "eslint": "^5.7.0",
    "husky": "^1.0.1",
    "lint-staged": "^7.3.0",
    "mocha": "^6.2.1"
  }
